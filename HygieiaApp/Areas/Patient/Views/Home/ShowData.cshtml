@model ApplicationUser



<div class="card-body p-3">

    <form method="post">
        <div class="border p-3  mt-4">
            <div class="row pb-2">
                <h2 class="text-primary">My Details</h2>
                <hr/>
            </div>
    
        <div class="mb-3">
            <label class="col-form-label" asp-for="@Model.FirstName"></label><br/>
            <input type="text" asp-for="@Model.FirstName" disabled class="form-control"/><br/>
            <label class="col-form-label" asp-for="@Model.LastName"></label><br/>
            <input type="text" asp-for="@Model.LastName" disabled class="form-control"/><br/>
            <label class="col-form-label" asp-for="@Model.Email"></label><br/>
            <input type="text" asp-for="@Model.Email" disabled class="form-control"/><br/>
            <label class="col-form-label">Doctor</label><br/>
         
            <a asp-controller="Home" asp-action="Index">Back to listing</a>
        </div>
        </div>
    </form>
</div>

